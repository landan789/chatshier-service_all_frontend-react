{
  "name": "react.chatshier",
  "version": "1.0.1",
  "private": true,
  "engines": {
    "node": "^8.9.4",
    "npm": "^5.6.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ -w -r",
    "build": "npm-run-all build-css build-js",
    "build-js": "react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/"
  },
  "dependencies": {
    "bootstrap": "4.1.0",
    "fullcalendar": "3.9.0",
    "jquery": "3.3.1",
    "jwt-decode": "2.2.0",
    "moment": "2.22.1",
    "node-sass-chokidar": "1.2.2",
    "npm-run-all": "4.1.2",
    "react": "16.3.2",
    "react-aux": "1.1.0",
    "react-dom": " 16.3.2",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.1.4",
    "react-switch": "2.3.2",
    "react-widgets": "4.2.6",
    "react-widgets-moment": "4.0.12",
    "reactstrap": "5.0.0",
    "redux": "4.0.0",
    "socket.io": "2.1.0",
    "swiper": "4.2.2"
  },
  "devDependencies": {
    "@types/react": "16.3.12",
    "@types/react-redux": "5.0.18",
    "@types/react-router-redux": "5.0.13",
    "@types/swiper": "4.2.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-standard": "3.1.0"
  }
}
